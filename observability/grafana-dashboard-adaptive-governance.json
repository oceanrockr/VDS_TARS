{
  "dashboard": {
    "title": "T.A.R.S. Adaptive Governance & Cognitive Federation",
    "tags": ["tars", "phase10", "cognitive", "governance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Cognitive Insight Generation Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(tars_cognitive_insights_generated_total[5m])",
            "legendFormat": "{{type}} - {{priority}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Insights/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Cognitive Recommendation Score",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "avg(tars_cognitive_recommendation_score)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Active Insights by Type",
        "type": "piechart",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum by (type) (tars_cognitive_insights_generated_total)",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Policy Adaptation Success Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(tars_policy_adaptation_success_total[5m])) / (sum(rate(tars_policy_adaptation_success_total[5m])) + sum(rate(tars_policy_adaptation_failures_total[5m])))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Active Policy Proposals",
        "type": "stat",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "tars_policy_proposals_active",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Consensus Reward Signal",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "tars_consensus_reward{component=\"total\"}",
            "legendFormat": "Total Reward",
            "refId": "A"
          },
          {
            "expr": "tars_consensus_reward{component=\"latency\"}",
            "legendFormat": "Latency Component",
            "refId": "B"
          },
          {
            "expr": "tars_consensus_reward{component=\"accuracy\"}",
            "legendFormat": "Accuracy Component",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Reward"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Meta-Optimizer Actions Distribution",
        "type": "bargauge",
        "gridPos": {"x": 0, "y": 16, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum by (action_type) (tars_consensus_optimization_actions_total)",
            "legendFormat": "{{action_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 8,
        "title": "Consensus Timeout Optimization",
        "type": "graph",
        "gridPos": {"x": 8, "y": 16, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "tars_consensus_timeout_ms",
            "legendFormat": "{{algorithm}} timeout",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Timeout (ms)"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Q-Learning Table Size",
        "type": "stat",
        "gridPos": {"x": 16, "y": 16, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "tars_consensus_q_table_size",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Ethical Fairness Model Accuracy",
        "type": "gauge",
        "gridPos": {"x": 16, "y": 20, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "tars_fairness_model_accuracy",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.80, "color": "yellow"},
                {"value": 0.90, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Fairness Predictions (Fair vs Unfair)",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 24, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum by (prediction) (tars_fairness_predictions_total)",
            "legendFormat": "{{prediction}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 12,
        "title": "Policy Validation Time",
        "type": "graph",
        "gridPos": {"x": 8, "y": 24, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tars_policy_validation_seconds_bucket[5m]))",
            "legendFormat": "P95 Validation Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(tars_policy_validation_seconds_bucket[5m]))",
            "legendFormat": "P50 Validation Time",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Validation Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 13,
        "title": "Insight Processing Latency",
        "type": "graph",
        "gridPos": {"x": 16, "y": 24, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tars_cognitive_insight_processing_seconds_bucket[5m]))",
            "legendFormat": "P95 Processing Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(tars_cognitive_insight_processing_seconds_bucket[5m]))",
            "legendFormat": "P50 Processing Time",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Processing Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 14,
        "title": "Policy Rollbacks",
        "type": "stat",
        "gridPos": {"x": 0, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(tars_policy_rollback_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 3, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 15,
        "title": "Insights Applied vs Rejected",
        "type": "timeseries",
        "gridPos": {"x": 6, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(tars_cognitive_insights_applied_total[5m]))",
            "legendFormat": "Applied",
            "refId": "A"
          },
          {
            "expr": "sum(rate(tars_cognitive_insights_generated_total[5m])) - sum(rate(tars_cognitive_insights_applied_total[5m]))",
            "legendFormat": "Rejected/Pending",
            "refId": "B"
          }
        ]
      },
      {
        "id": 16,
        "title": "Top Policy Violations (Last 24h)",
        "type": "table",
        "gridPos": {"x": 18, "y": 32, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "topk(10, sum by (policy_id) (increase(tars_policy_decisions_total{decision=\"deny\"}[24h])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {"policy_id": 0, "Value": 1},
              "renameByName": {"policy_id": "Policy ID", "Value": "Violations"}
            }
          }
        ]
      },
      {
        "id": 17,
        "title": "Recommendation Requests Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 40, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rate(tars_cognitive_recommendation_requests_total[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests"},
          {"format": "short"}
        ]
      },
      {
        "id": 18,
        "title": "Model Training Runs",
        "type": "stat",
        "gridPos": {"x": 12, "y": 40, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(tars_fairness_training_runs_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 19,
        "title": "Success Criteria Dashboard",
        "type": "table",
        "gridPos": {"x": 18, "y": 40, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tars_cognitive_insight_processing_seconds_bucket[5m])) <= 5",
            "format": "table",
            "instant": true,
            "legendFormat": "Insight Latency â‰¤ 5s",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
